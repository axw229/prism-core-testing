{
  "name": "@prism/toolkit",
  "version": "4.0.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "publishConfig": {
    "registry": "https://artifactory.sherwin.com/artifactory/api/npm/sherwin-npm-virtual/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "TOOLKIT_PROTECT_CLASS=p3m-p7d start-storybook -p 6006",
    "lint": "eslint src --cache",
    "cleancache": "start-storybook --ci --no-manager-cache &> /dev/null & exit 1 & echo cache cleaned",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cypress": "npm run lint && start-server-and-test 'start-storybook -p 6006 --ci &> /dev/null' 6006 'cypress run'",
    "cypress": "start-server-and-test 'start-storybook -p 6006 --ci &> /dev/null' 6006 'cypress open'",
    "testtab": "start-server-and-test 'start-storybook -p 6006 --ci &> /dev/null' 6006 'node .storybook/addons/testTab/getTestReports.js'",
    "predeploy": "npm run build && npm run testtab",
    "deploy": "storybook-to-ghpages -e ./public",
    "build": "TOOLKIT_PROTECT_CLASS=p3m-p7d build-storybook -o ./public",
    "build-rollup-facets": "rollup -c rollup.config.js",
    "build-rollup-protected": "TOOLKIT_PROTECT_CLASS=p3m-p7d rollup -c rollup.config.js",
    "ci": "TOOLKIT_PROTECT_CLASS=p3m-p7d start-storybook -p 6006 --ci & sleep 60 && cypress run && node .storybook/addons/testTab/getTestReports.js",
    "check-types": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.2",
    "@fortawesome/pro-duotone-svg-icons": "6.1.2",
    "@fortawesome/pro-light-svg-icons": "6.1.2",
    "@fortawesome/pro-regular-svg-icons": "6.1.2",
    "@fortawesome/pro-solid-svg-icons": "6.1.2",
    "@fortawesome/pro-thin-svg-icons": "6.1.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@rollup/plugin-replace": "4.0.0",
    "@testing-library/jest-dom": "5.16.4",
    "@thoughtbot/tailwindcss-aria-attributes": "0.2.0",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "get-element-relative-offset": "1.0.0",
    "heic2any": "0.0.2",
    "lodash": "4.17.21",
    "memoizee": "0.4.14",
    "react-aria-live": "2.0.5",
