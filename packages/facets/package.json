{
  "name": "@prism/facets",
  "version": "2.4.1",
  "scripts": {
    "postinstall": "yarn run create-empty-prism-manifest && yarn run create-empty-embed-file",
    "create-empty-prism-manifest": "node build-scripts/utilities/create-empty-prism-manifest.js",
    "create-empty-embed-file": "node build-scripts/utilities/create-empty-embed-file.js",
    "clear-embed-output": "node build-scripts/utilities/clear-embed-output.js",
    "copy-embed-to-dist": "node build-scripts/utilities/copy-embed-output-to-dist.js",
    "clear-prism-build-cache": "node build-scripts/utilities/clear-build-cache.js",
    "preembed-cleanstart": "yarn run clear-embed-output",
    "embed-cleanstart": "yarn run embed-start",
    "preembed-start": "yarn run create-empty-embed-file",
    "embed-start": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config webpack/webpack.embed.dev.js",
    "embed-build": "cross-env NODE_ENV=production webpack --config webpack/webpack.embed.prod.js",
    "templates-start": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config webpack/webpack.templates.dev.js",
    "templates-build": "cross-env NODE_ENV=production webpack --config webpack/webpack.templates.prod.js",
    "preprism-cleanstart": "yarn run clear-prism-build-cache",
    "prism-cleanstart": "yarn start",
    "preprism-start": "yarn run create-empty-prism-manifest",
    "prism-start": "cross-env ML_API_URL=https://develop-prism-ml-api.ebus.swaws NODE_ENV=development SMARTMASK_ENABLED=1 FIREBASE_AUTH_ENABLED=1 webpack-dev-server --mode development --open --hot --config webpack/webpack.prism.dev.js",
    "precleanstart": "yarn run clear-embed-output && yarn run clear-prism-build-cache && yarn run create-empty-prism-manifest",
    "cleanstart": "yarn run start",
    "start": "cross-env yarn run prism-start & yarn run embed-start & yarn run templates-start",
    "start:mock": "MOCK_API=1 yarn run start",
    "preprism-build": "yarn run clear-prism-build-cache",
    "prism-build": "cross-env NODE_ENV=production FIREBASE_AUTH_ENABLED=1 webpack --config webpack/webpack.prism.prod.js",
    "prebuild": "yarn run clear-embed-output",
    "build": "yarn run prism-build && yarn run embed-build && yarn run templates-build && yarn run build-storybook",
    "postbuild": "yarn run copy-embed-to-dist",
    "build:publish": "cross-env API_URL=https://prism-api.sherwin-williams.com WEB_URL=https://replatform-prism-web.ebus.swaws webpack --config webpack/webpack.prism.publish.js",
    "test": "jest --coverage",
    "test:changed": "jest --changedSince=develop --coverage",
    "test:ci": "jest --ci",
    "test:advanced": "jest --coverage --config=./jestAdvanced.config.js",
    "test-watch": "jest --watch --coverage",
    "preflow-check": "yarn run flow-install",
    "flow-check": "flow check",
    "flow-check-verbose": "flow check --show-all-errors",
    "flow-install": "flow-typed install",
    "__x__prepare": "yarn run build:publish",
    "lint": "eslint src/ --ext .js,.ts,.jsx,.tsx --quiet",
    "lint-fix": "yarn run lint --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist/storybook"
  },
  "main": "dist/index.js",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-canvas-mock",
      "<rootDir>/setup/setupFile.js"
    ],
    "moduleNameMapper": {
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-touch-backend$": "react-dnd-touch-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs",
      "^constants(.*)$": "<rootDir>/src/constants$1",
      "^variables$": "<rootDir>/src/shared/variablesExport.js",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.((s?css)|less)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^config/(.*)$": "<rootDir>/config/$1",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1"
    },
    "globals": {
      "API_PATH": "http://localhost",
      "APP_NAME": "PRISM",
      "APP_VERSION": "1.3.3",
      "BASE_PATH": "localhost",
      "ENV": "development",
      "ML_API_URL": "localhost",
      "WEBPACK_CONSTANTS": {},
      "VAR_NAMES": {},
      "VAR_VALUES": {}
    }
  },
