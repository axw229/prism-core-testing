[ignore]
.*/node_modules/.*
.*/__tests__/.*

[include]

[libs]
src/shared/types/lib

[lints]

[options]
react.runtime=automatic
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/packages/facets/src/shared/types/CSSFlowStub.js'
