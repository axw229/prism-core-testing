module.name_mapper='^src\/\(.*\)$' -> '<PROJECT_ROOT>/packages/facets/src/\1'
module.name_mapper='^srcEmbed\/\(.*\)$' -> '<PROJECT_ROOT>/packages/facets/src-embed/\1'
module.name_mapper='^srcTemplates\/\(.*\)$' -> '<PROJECT_ROOT>/packages/facets/src-templates/\1'
module.name_mapper='^embedOuput\/\(.*\)$' -> '<PROJECT_ROOT>/packages/facets/embed-output/\1'
module.name_mapper='^embedWorking\/\(.*\)$' -> '<PROJECT_ROOT>/packages/facets/embed-working/\1'
module.name_mapper='^__mocks__\/\(.*\)$' -> '<PROJECT_ROOT>/packages/facets/__mocks__/\1'

[strict]
