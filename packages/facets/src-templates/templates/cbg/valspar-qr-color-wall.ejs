<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <base href="/">
  <title>Valspar QR Color Wall</title>
  <style>
    body {
      margin: 0;
    }

    @media only screen and (min-width: 1280px) {
      body {
        width: 1280px;
      }
    }
  </style>
</head>

<body>

  <div
    id="prism-embed-root"
    data-prism-facet="ColorWallChunkChipFacet"
    data-brand="valspar"
    data-color-wall-bg-color="#FFF"
    data-auto-height
    data-chunk-mini-map
    data-language="en-US"
    data-route-type="hash"
  ></div>

  <script src='<%= BASE_PATH %>/embed.js'></script>
  <script>
    (() => {
    const baseHostUrl = window.location.origin
    const kebabCase = (string) => string.replace(/[\W_]+/g, '-')
    window.PRISM.embed(document.getElementById('prism-embed-root'), {
      wallBanner: 'https://sherwin.scene7.com/is/image/sw/valspar-qr-banner?fmt=png&qlt=95',
      colorDetailPageRoot: (color) => `${baseHostUrl}/inspiration/colors/${color.colorFamilyNames[0]}/${kebabCase(color.name)}-${color.colorNumber}`.toLowerCase() // example
    })
  })()
  </script>

</body>

</html>
