  brandKey: string,
  colorFamilyNames: Array<string>,
  colorNumber: string,
  coordinatingColors: {
    coord1ColorId: number,
    coord2ColorId: number,
    whiteColorId: number
  },
  csshsl: string,
  cssrgb: string,
  description: Array<string>,
  green: number,
  hex: string,
  hue: number,
  id: number,
  isDark: boolean,
  isExterior: boolean,
  isInterior: boolean,
  lightness: number,
  name: string,
  opacity: string, // ex: "SW1234", whatever that means
  red: number,
  rgb: number,
  saturation: number,
  smis: string,
  storeStripLocator?: string
