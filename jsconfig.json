{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "__mocks__": ["src/__mocks"],
      "config": ["src/config"],
      "constants": ["src/constants"],
      "embedOutput": ["embed-output"],
      "embedWorking": ["embed-working"],
      "src": ["src"],
      "variables": ["src/shared/withBuild/variableDefs.js"],
    }
  },
  "exclude": [
    "node_modules",
    "embed-output",
    "embed-working",
    "dist",
    ".config",
    "__mocks__/data",
    "flow-typed"
  ]
}