    "@fortawesome/pro-regular-svg-icons": "5.11.2",
    "@fortawesome/pro-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.6",
    "assert": "2.0.0",
    "axios": "0.19.0",
    "core-js": "2.6.5",
    "delta-e": "0.0.7",
    "focus-within-polyfill": "^5.0.7",
    "immutability-helper": "3.0.1",
    "lodash": "4.17.15",
    "react-aria-live": "2.0.5",
    "react-aria-menubutton": "6.2.0",
    "react-dnd-cjs": "9.5.1",
    "react-dnd-html5-backend-cjs": "9.5.1",
    "react-ga": "2.7.0",
    "react-inlinesvg": "0.8.4",
    "react-intl": "3.10.0",
    "react-redux": "7.1.1",
