{
  "name": "prism",
  "version": "1.4.0",
  "scripts": {
    "precleanstart": "node build-scripts/utilities/clear-build-cache.js",
    "cleanstart": "npm start",
    "start": "cross-env NODE_ENV=development API_URL=https://develop-prism-api.ebus.swaws ML_API_URL=http://localhost ENTRY=$ENTRY webpack-dev-server --mode development --open --hot --config webpack.dev.js",
    "prebuild": "node build-scripts/utilities/clear-build-cache.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build:publish": "cross-env API_URL=https://prism-api.sherwin-williams.com WEB_URL=https://replatform-prism-web.ebus.swaws webpack --config webpack.publish.js",
    "test": "jest --coverage",
    "test:changed": "jest --changedSince=develop --coverage",
    "test:ci": "jest --ci",
    "test-watch": "jest --watch --coverage",
    "preflow-check": "npm run flow-install",
    "flow-check": "flow check",
    "flow-check-verbose": "flow check --show-all-errors",
    "flow-install": "flow-typed install",
    "prepare": "npm run build:publish",
    "lint": "eslint src/ --ext .js,.ts,.jsx,.tsx",
    "lint-fix": "npm run lint --fix"
  },
  "main": "dist/index.js",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-canvas-mock",
      "<rootDir>/setup/setupFile.js"
    ],
    "moduleNameMapper": {
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-touch-backend$": "react-dnd-touch-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs",
      "^constants(.*)$": "<rootDir>/src/constants$1",
      "^variables$": "<rootDir>/src/shared/variablesExport.js",
      "\\.((s?css)|less)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^config/(.*)$": "<rootDir>/config/$1",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1"
    },
    "globals": {
      "API_PATH": "http://localhost",
      "APP_NAME": "PRISM",
      "APP_VERSION": "1.3.3",
      "BASE_PATH": "localhost",
      "ENV": "development",
      "ML_API_URL": "localhost",
      "WEBPACK_CONSTANTS": {},
      "VAR_NAMES": {},
      "VAR_VALUES": {}
    }
  },
  "dependencies": {
    "@ctrl/tinycolor": "2.6.0",
    "@formatjs/intl-pluralrules": "1.3.0",
    "@formatjs/intl-relativetimeformat": "4.2.0",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
