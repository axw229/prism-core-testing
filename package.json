    "react-tabs": "3.0.0",
    "react-transition-group": "4.3.0",
    "react-virtualized": "9.21.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "scroll": "3.0.1"
  },
  "peerDependencies": {
    "react": "16.10.2",
    "react-dom": "16.10.2"
  },
