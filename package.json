    "react-dnd": "7.4.3",
    "react-dnd-html5-backend": "7.4.0",
    "react-dom": "16.8.5",
    "react-ga": "2.5.7",
    "react-inlinesvg": "0.8.4",
    "react-intl": "2.8.0",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-tabs": "3.0.0",
    "react-transition-group": "2.7.1",
    "react-virtualized": "9.21.0",
    "redux": "4.0.1",
