{
  "name": "prism",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "start": "cross-env API_URL=https://dev-prism-api.ebus.swaws WEB_URL=http://localhost:8080 webpack-dev-server --mode development --open --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --coverage",
    "flow-check": "flow check",
    "test-watch": "jest --watch",
    "postinstall": "flow-typed install"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setup/setupEnzyme.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^constants(.*)$": "<rootDir>/src/constants$1",
      "^variables$": "<rootDir>/src/shared/variables.js",
      "\\.((s?css)|less)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1"
    },
    "globals": {
      "API_PATH": "localhost",
      "BASE_PATH": "localhost"
    }
  },
  "dependencies": {
    "@babel/polyfill": "7.4.0",
    "@ctrl/tinycolor": "2.4.0",
